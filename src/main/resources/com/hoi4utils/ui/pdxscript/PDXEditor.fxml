<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.Tab?>
<?import javafx.scene.control.TabPane?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>

<AnchorPane fx:id="rootAnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/${javafx.version}" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.hoi4utils.ui.pdxscript.PDXEditorController">
   <children>
      <TabPane tabClosingPolicy="UNAVAILABLE">
        <tabs>
          <Tab text="SkorpPane">
            <content>
              <AnchorPane fx:id="rootSkorpPane" prefHeight="400.0" prefWidth="600.0" />
            </content>
          </Tab>
          <Tab text="ThiccPane">
               <content>
                  <AnchorPane>
                     <children>
                        <ScrollPane>
                           <content>
                              <BorderPane>
                                 <center>
                                    <GridPane prefHeight="765.0" prefWidth="600.0" BorderPane.alignment="CENTER">
                                      <columnConstraints>
                                          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                                          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="200.0" />
                                          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                                        <ColumnConstraints hgrow="SOMETIMES" maxWidth="1.7976931348623157E308" minWidth="10.0" prefWidth="200.0" />
                                        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                                      </columnConstraints>
                                      <rowConstraints>
                                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                                          <RowConstraints maxHeight="71.0" minHeight="10.0" prefHeight="57.0" vgrow="SOMETIMES" />
                                          <RowConstraints maxHeight="71.0" minHeight="10.0" prefHeight="57.0" vgrow="SOMETIMES" />
                                          <RowConstraints maxHeight="35.0" minHeight="9.0" prefHeight="23.0" vgrow="SOMETIMES" />
                                        <RowConstraints minHeight="10.0" prefHeight="100.0" vgrow="SOMETIMES" />
                                        <RowConstraints maxHeight="31.0" minHeight="0.0" prefHeight="30.0" vgrow="SOMETIMES" />
                                          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                                          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                                          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                                          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                                          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                                      </rowConstraints>
                                       <children>
                     
                                                           <Label text="Continue if Invalid" GridPane.columnIndex="0" GridPane.rowIndex="9" />
                     
                                                           <Label text="Cancel if Invalid" GridPane.columnIndex="3" GridPane.rowIndex="8" />
                     
                                                           <!-- Mutually Exclusive Field -->
                                         <Label text="Mutually Exclusive" GridPane.columnIndex="1" GridPane.rowIndex="5" />
                                         <!-- ID Field -->
                                         <Label alignment="TOP_CENTER" contentDisplay="TOP" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" text="ID" GridPane.columnIndex="1" GridPane.rowIndex="1" />
                                         <TextField fx:id="idField" onAction="#handleFocusID" GridPane.columnIndex="1" GridPane.rowIndex="1" />
                     
                                               <!-- Icon Field -->
                                         <Label alignment="TOP_CENTER" contentDisplay="TOP" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" text="Icon" GridPane.columnIndex="1" GridPane.rowIndex="2" />
                                         <TextField fx:id="iconField" onAction="#handleFocusIcon" GridPane.columnIndex="1" GridPane.rowIndex="2" />
                     
                                                     <!-- X and Y Coordinate Fields -->
                                         <Label alignment="TOP_CENTER" maxHeight="1.7976931348623157E308" text="X" GridPane.columnIndex="3" GridPane.rowIndex="1" />
                     
                                                           <Label alignment="BOTTOM_CENTER" maxHeight="1.7976931348623157E308" text="Y" GridPane.columnIndex="3" GridPane.rowIndex="1" />
                     
                                                           <!-- Prerequisite Field -->
                                         <Label alignment="CENTER" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" text="Prerequisite" GridPane.columnIndex="1" GridPane.rowIndex="3" />
                     
                                                           <!-- Relative Position Field -->
                                         <Label text="Relative Position" GridPane.columnIndex="1" GridPane.rowIndex="6" />
                     
                                                           <!-- Post Field -->
                                         <Label text="Post" GridPane.columnIndex="0" GridPane.rowIndex="7" />
                     
                                                           <!-- Availability and Cancellation Options -->
                                         <Label text="Available if Captured" GridPane.columnIndex="0" GridPane.rowIndex="8" />
                                       </children>
                                    </GridPane>
                                 </center>
                              </BorderPane>
                           </content>
                        </ScrollPane>
                     </children>
                  </AnchorPane>
               </content>
          </Tab>
        </tabs>
      </TabPane>
   </children>
</AnchorPane>
