		x = 1
		y = 0
		cost = 5.0
		available_if_capitulated = true

		ai_will_do = {
			factor = 1.0
			modifier = {
				add = 2.0
				OR = {
					has_completed_focus = SMA_Elections_Will_Continue
					has_completed_focus = SMA_Wickliffes_Coup
				}
			}
			modifier = {
				add = 5.0
				date > 1936.4.1
			}
		}

		completion_reward = {
			if = {
				limit = {
					capital_scope = {
						free_building_slots = {
							building = air_base
							size > 1
						}
					}
				}
				199 = {
					add_extra_state_shared_building_slots = 2
				}
			}
			if = {
				limit = {
					capital_scope = {
						free_building_slots = {
							building = air_base
							size > 1
						}
					}
				}
				3 = {
					add_extra_state_shared_building_slots = 2
				}
			}
		}
	}
