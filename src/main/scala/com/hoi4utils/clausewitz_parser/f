#Focus for Unify New England
	focus = {
		id = SMA_unifynewengland
		icon = GFX_focus_new_england_flag_democratic_banner
		x = 0
		y = 1
		relative_position_id = SMA_The_Curley_Effect
		cost = 10.0
		available_if_capitulated = true
		prerequisite = {
			focus = [null]
		}
		ai_will_do = {
			factor = 200.0
		}
		available = {
			OR = {
				has_government = longism
				AND = {
					has_government = neutrality
					fascism < 0.25
					communism < 0.25
